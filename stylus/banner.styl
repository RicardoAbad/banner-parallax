//Animations

.wave
	animation wave 4s 0.1s infinite linear
	.wave
		animation wave 6s 0.2s infinite linear
	
@keyframes wave
	0%
		transform rotateZ(0deg) translate3d(0, 10%, 0) rotateZ(0deg)
	100%
		transform rotateZ(360deg) translate3d(0, 10%, 0) rotateZ(-360deg)
			
.wave-slow
	animation wave-slow 15s infinite linear

@keyframes wave-slow 
	0% 
		transform rotateZ(0deg) translate3d(0, 3%, 0) rotateZ(0deg)
	100% 
		transform rotateZ(359deg) translate3d(0, 3%, 0) rotateZ(-359deg)

.wave-medium
	animation wave-medium 11s infinite linear

@keyframes wave-medium
	0% 
		transform rotateZ(0deg) translate3d(0, 3%, 0) rotateZ(0deg)
	100%
		transform rotateZ(359deg) translate3d(0, 3%, 0) rotateZ(-359deg)
		
.wave-scale
	animation wave-scale 6s infinite linear
	transform-origin 50% 50%
	
@keyframes wave-scale
	0%
		transform rotateZ(0deg) translate3d(0, 15%, 0) rotateZ(0deg) scale(1)
	50%
		transform rotateZ(180deg) translate3d(0, 15%, 0) rotateZ(-180deg) scale(.7)
	100%
		transform rotateZ(360deg) translate3d(0, 15%, 0) rotateZ(-360deg) scale(1)



.lighthouse
	animation lighthouse 6s 0.1s infinite alternate cubic-bezier(0.455, 0.03, 0.515, 0.955)
	transform-origin 50% -50%
	
@keyframes lighthouse 
	0% 
		transform translate3d(5%, 0, 0) rotateZ(2deg)
	100% 
		transform translate3d(-5%, 0, 0) rotateZ(-2deg)
	
.swing
	animation swing 4s infinite alternate cubic-bezier(0.455, 0.03, 0.515, 0.955)
	transform-origin 50% 90%
	
@keyframes swing 
	0% 
		transform rotateZ(2deg) translateZ(0)
	100% 
		transform rotateZ(-2deg) translateZ(0)
	
.bouncer
